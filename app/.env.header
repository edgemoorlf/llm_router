# Global rate limiting
RATE_LIMIT_TPM=30000
RATE_LIMIT_ENABLED=true

# Routing strategy (priority, round_robin, weighted, least_loaded, failover, model_specific)
API_ROUTING_STRATEGY=round_robin

# Feishu alerting
FEISHU_WEBHOOK_URL=https://open.feishu.cn/open-apis/bot/v2/hook/b814d703-482f-4ec4-8657-837b7dda9433

# Legacy single-instance configuration (used only if API_INSTANCES is empty)
API_KEY=your-fallback-api-key
API_BASE=https://your-fallback-resource.openai.azure.com
API_VERSION=2023-07-01-preview
API_PROVIDER_TYPE=azure

# Model mappings (global, used if instance doesn't have a specific mapping)
MODEL_MAP_GPT4O=gpt-4o
MODEL_MAP_GPT35TURBO=gpt-35-turbo
